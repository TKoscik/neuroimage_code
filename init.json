{
    "version": "0.0.0.0",
    "export_paths": {
        "DIR_DB": "/Dedicated/inc_database",
        "DIR_TMP": "/Shared/inc_scratch",
        "DIR_TEMPLATE": "/Dedicated/inc_database/templates",
        "DIR_LUT": "${DIR_INC}/lut"
        "DIR_IMPORT": "/Dedicated/inc_scratch/import",
        "DIR_QC": "/Dedicated/inc_scratch/qc",
        "DIR_PINC": "/Shared/pinc/sharedopt/apps"
    },
    "function_alias": [
        "anat", "cron", "dicom", "dwi", "export", "func", "generic", "log"
    ],
    "software": {
        "OpenBLAS": {
            "hostname": "argon",
            "version": "0.2.19",
            "command": ["module load ${SW_NAME}/${VERSION}"]
        },
        "R": {
            "hostname": "argon",
            "version": "3.5.1",
            "command": ["module load ${SW_NAME}/${VERSION}"]
        },
        "afni": {
            "hostname": "all",
            "version": "18.2.04",
            "command": ["source ${DIR_PINC}/sourcefiles/${SW_NAME}_source.sh ${VERSION}",
                        "export AFNI_NIFTI_TYPE_WARN=NO"]
        },
        "ants": {
            "hostname": "all",
            "version": "2.3.1",
            "command": ["source ${DIR_PINC}/sourcefiles/${SW_NAME}_source.sh ${VERSION}"]
        },
        "fsl": {
            "hostname": "all",
            "version": "6.0.1_multicore",
            "command": ["source ${DIR_PINC}/sourcefiles/${SW_NAME}_source.sh ${VERSION}"]
        },
        "freesurfer": {
            "hostname": "all",
            "version": "7.1.0",
            "command": ["export FREESURFER_HOME=/Shared/pinc/sharedopt/apps/freesurfer/Linux/x86_64/${version}",
                        "export FS_LICENSE=/Shared/inc_scratch/license/freesurfer/${version}/license.txt",
                        "source ${FREESURFER_HOME}/FreeSurferEnv.sh"]
        },
        "anaconda3": {
            "hostname": "all",
            "version": "2019.10",
            "command": ["source ${DIR_PINC}/sourecfiles/${SW_NAME}_source.sh ${VERSION}"]
        },
        "DCM2NIIX": {
            "hostname": "all",
            "version": "1.0.20200331",
            "command": ["export ${SW_NAME}=/Shared/pinc/sharedopt/apps/dcm2niix/Linux/x86_64/${VERSION}/dcm2niix"]
        },
        "DSISTUDIO": {
            "hostname": "all",
            "version": "20200122",
            "command": ["export ${SW_NAME}=/Shared/pinc/sharedopt/apps/DSI_Studio/Linux/x86_64/${VERSION}/dsi_studio"]
        },
        "SNAP": {
            "hostname": "all",
            "version": "3.8.0-20190612",
            "command": ["export ${SW_NAME}=/Shared/pinc/sharedopt/apps/itk-snap/Linux/x86_64/${VERSION}/bin/itksnap"]
        },
        "SNAPQT": {
            "hostname": "all",
            "version": "3.8.0-qt4-20190612",
            "command": ["export ${SW_NAME}=/Shared/pinc/sharedopt/apps/itk-snap/Linux/x86_64/${VERSION}/bin/itksnap"]
        }
    }
}
